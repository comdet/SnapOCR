{    
    "name": "SnapOCR",
    "version": "0.0.13",
    "manifest_version": 2,
    "description": "Image to Text just click and crop",
    "icons": {
        "16": "images/ic-16.png",
        "128": "images/ic-128.png"
    },
    "background": {
        "scripts": [            
            "scripts/background.js"
        ]
    },
    "page_action": {
        "default_icon": {
            "19": "images/ic-19.png",
            "38": "images/ic-38.png"
        },
        "default_title": "SnapOCR"
    },
    "omnibox": {
        "keyword": "SnapOCR"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*",
        "https://translate.google.com/"
    ],
    "web_accessible_resources": [
        "web_images/*",
        "styles/*"
    ]
}